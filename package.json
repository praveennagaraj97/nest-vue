{
  "name": "simple-todos",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test": "cross-env NODE_ENV=test nyc mocha --require test/index.ts --opts test/mocha.opts",
    "prestart": "rimraf dist && npm run build && tsc -p server",
    "build": "nuxt build"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.5.4",
    "@nestjs/websockets": "^4.5.8",
    "@nuxtjs/axios": "^4.5.2",
    "@nuxtjs/dotenv": "^1.1.0",
    "@types/sequelize": "^4.27.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "continuation-local-storage": "^3.2.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "indicative": "^3.0.6",
    "izitoast": "^1.2.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.17",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "nestjs-extensions": "^0.8.0",
    "nuxt": "^1.1.1",
    "nuxtjs-extensions": "^1.0.6",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "sequelize": "^4.31.2",
    "sequelize-typescript": "^0.6.2",
    "sharp": "^0.19.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "socket.io-redis": "^5.2.0",
    "source-map-support": "^0.5.1",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "typeorm": "^0.1.9",
    "uuid": "^3.2.1",
    "vee-validate": "^2.0.3",
    "vue-notifications": "^0.9.0",
    "vue-socket.io": "^2.1.1-a",
    "vuetify": "^1.0.0-beta.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/bluebird": "^3.5.19",
    "@types/chai": "^4.1.0",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.0",
    "@types/express-session": "^1.15.6",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.92",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^2.2.46",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/mysql": "^2.15.2",
    "@types/node": "^8.5.8",
    "@types/redis": "^2.8.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/rimraf": "^2.0.2",
    "@types/sharp": "^0.17.6",
    "@types/sinon": "^4.1.3",
    "@types/socket.io": "^1.4.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/socket.io-redis": "^1.0.23",
    "@types/source-map-support": "^0.4.0",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-plugin-vue": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.11",
    "nyc": "^11.4.1",
    "sinon": "^4.1.6",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "test/**/*"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/*.e2e-spec.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "report-dir": "./coverage"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "env": {
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 8
    },
    "rules": {
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off",
      "vue/max-attributes-per-line": 0
    }
  },
  "nodemonConfig": {
    "watch": [
      "server",
      "nuxt.config.js"
    ],
    "ext": "js ts",
    "ignore": [
      "server/**/*.spec.ts",
      "server/build/"
    ],
    "exec": "ts-node --type-check --project server server"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
